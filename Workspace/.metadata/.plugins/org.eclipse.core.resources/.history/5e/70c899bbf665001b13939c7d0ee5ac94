
public class Methodentester {

	public static void main(String[] args) {
		int[][] tester = {{1,2}, {3,4}};
		//System.out.println(tester[0][1]);
		//System.out.println(tester[1]);
		
		for(int i = 0; i < 2; i++) {
			for(int j = 0; j < 2; j++) {
				System.out.print(tester[i][j] + " ");
			}
		}

	}
	
	public static int[][] fuellen(int[][]karten, int spalte) {
		int[][] ret = null;
		
		if (karten == null) {
			int n = 0;
			karten = new int[3][7];
			for (int i = 0; i < 3; i++) {
				for (int j = 0; j < 7; j++) {
					karten[i][j] = n+1;
					n++;
				}
			}
			ret = karten;
		}
		
		return ret;
	}
}
